
<!-- Footer -->
<div id="footer">
	<p class="tagline">
		Page generated by <a href="http://www.munin-monitoring.org/">Munin</a>
		version <TMPL_VAR NAME='MUNIN_VERSION'>
		<br>
		@ <TMPL_VAR NAME='TIMESTAMP'>
	</p>
</div>


<!-- Modal Zoom Dialog-->
<div class="modal fade" id="ZoomModal" tabindex="-1" role="dialog" aria-labelledby="ZoomModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">

			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
				<h3 class="modal-title" id="ZoomModalLabel">...</h3>
			</div>

			<div class="modal-body" id="ZoomModalBody">...</div>

			<div class="modal-footer">
				<b>From: </b>
				<input type="text" class="datepicker" id="start_date" data-date-autoclose="1"
						data-date-format="dd/mm/yyyy" data-date-week-start="1"
						data-date-today-btn="linked" data-date-today-highlight="1">
				<b>To: </b>
				<input type="text" class="datepicker" id="stop_date" data-date-autoclose="1"
						data-date-format="dd/mm/yyyy" data-date-week-start="1"
						data-date-today-btn="linked" data-date-today-highlight="1">

				<div class="btn-group dropup">
					<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
						Period <span class="caret"></span>
					</button>
					<ul class="dropdown-menu" role="menu">
						<li><a href="javascript:FixedPeriod(1)">Last hour</a></li>
						<li><a href="javascript:FixedPeriod(6)">Last 6 hours</a></li>
						<li><a href="javascript:FixedPeriod(24)">Last 24 hours</a></li>
						<li><a href="javascript:FixedPeriod(48)">Last 2 days</a></li>
						<li><a href="javascript:FixedPeriod(168)">Last week</a></li>
						<li><a href="javascript:FixedPeriod(336)">Last 2 weeks</a></li>
						<li><a href="javascript:FixedPeriod(672)">Last month</a></li>
						<li><a href="javascript:FixedPeriod(8064)">Last year</a></li>
					</ul>
				</div>

				<button type="button" class="btn btn-primary" onclick="ReloadZoomImg()">Generate</button>
				<button type="button" class="btn" data-dismiss="modal">Close</button>
			</div>


		</div>
	</div>
</div>
